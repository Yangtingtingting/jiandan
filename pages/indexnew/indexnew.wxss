/* pages/indexnew/indexnew.wxss */
.wrap{
    background: #f5f5f5;
}
.navtext {
    font-size: 28rpx;
    line-height: 88rpx;
    color: #111111;
    background-image: url("data:image/png;base64,");
    background-size: 40rpx;
    background-repeat: no-repeat;
    background-position: 58rpx 24rpx;
    padding-right: 34rpx;
}

.nacsearch {
    padding: 16rpx 24rpx;
    background: #FFCF08;
    display: flex;
    justify-content: space-between;
}

.searchinput {
    width: 80%;
}

.searchinput input {
    background-color: #fff;
    height: 58rpx;
    border-radius: 32rpx;
    font-size: 24rpx;
    box-sizing: border-box;
    margin-top: 15rpx;
    background-image: url("data:image/png;base64,");
    background-size: 32rpx;
    background-repeat: no-repeat;
    background-position: 19rpx 15rpx;
    padding-left: 68rpx;
}

.searchplace {
    font-size: 24rpx;
    line-height: 58rpx;
    color: #AAAAAA;
}

.swiperbox {
    width: 100%;
    margin: 0 auto;
}

.contentbox {
    background-color: #f5f5f5;
    border-top-left-radius: 24rpx;
    border-top-right-radius: 24rpx;
    position: relative;
    top: -16rpx;
    padding: 24rpx;
    padding-bottom: 0px;
}

.bannerswiper image {
    width: 100%;
    height: 350rpx;
}

.bannerswiper {
    border-radius: 16rpx;
}

.parttitle {
    font-size: 16px;
    line-height: 46px;
    color: #111111;
    font-weight: 400;
    position: relative;
}

.parttitle::after {
    content: "";
    width: 64px;
    height: 4px;
    background: linear-gradient(180deg, #FFCF08 0%, #F2B100 100%);
    opacity: 0.5;
    position: absolute;
    bottom: 24rpx;
    left: 0px;
}

.partitem {
    background-color: #fff;
    padding: 24rpx;
    border-radius: 8rpx;
    margin-bottom: 24rpx;
}

.partitemtitle {
    font-size: 18px;
    font-weight: bold;
    line-height: 25px;
    color: #111111;
    display: flex;
    justify-content: space-between;
}

.partitemtitle span {
    font-size: 36rpx;
    font-weight: bold;
    line-height: 50rpx;
    color: #E4630D;
}

.partitemadress {
    font-size: 28rpx;
    line-height: 80rpx;
    color: #555555;
    display: flex;
    background-image: url("data:image/png;base64,");
    background-size: 32rpx;
    background-repeat: no-repeat;
    background-position: 0px 23rpx;
    padding-left: 40rpx;
}
.partitemadress span{
    background-image: url("data:image/png;base64,");
    background-size: 32rpx;
    background-repeat: no-repeat;
    background-position: 2rpx 23rpx;
    padding-left: 40rpx;
    margin-left: 32rpx;
    width: 246rpx;
    display: block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.partitembtn {
    width: 128rpx;
    height: 50rpx;
    background: #FFFFFF;
    border: 1px solid #17B456;
    opacity: 1;
    border-radius: 8rpx;
    font-size: 24rpx;
    line-height: 50rpx;
    color: #17B456;
    text-align: center;
}
.readmore{
    font-size: 28rpx;
    line-height: 120rpx;
    color: #555555;
    text-align: center;
    background-image: url("data:image/png;base64,");
    background-size: 40rpx;
    background-repeat: no-repeat;
    background-position:436rpx 41rpx;
}
.no-login-user {
    width: 130rpx;
    height: 130rpx;
    background: #f9f9f9;
    border-radius: 50%;
    overflow: hidden;
    margin: 60rpx auto;
}
.no-login-user image {
    width: 100%;
    display: block;
}