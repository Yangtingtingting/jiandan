/* pages/position_detail/position_detail.wxss */
.wrap {
    width: 100%;
}

.topcontent {
    padding: 24rpx;
    padding-bottom: 0px;
}

.pos_head {
    border-bottom: 1px solid #EEEEEE;
}

.pos_headtop {
    display: flex;
    justify-content: space-between;
    margin-top: 32rpx;
}

.pos_title {
    font-size: 48rpx;
    font-weight: bold;
    line-height: 66rpx;
    color: #333333;
}

.pos_price {
    font-size: 36rpx;
    font-weight: bold;
    line-height: 50rpx;
    color: #E4630D;
    margin-top: 8rpx;
}

.pos_msg {
    display: flex;
    justify-content: space-between;
    margin-top: 24rpx;
    padding-bottom: 20rpx;
}

.pos_msg view {
    font-size: 28rpx;
    line-height: 40rpx;
    color: #555555;
    margin-right: 48rpx;
    background-size: 32rpx;
    background-repeat: no-repeat;
    /* X,Y(left,right) */
    background-position: 0px 5rpx;
    padding-left: 20px;
}

.pos_msg view:nth-child(1) {
    max-width: 257rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    background-image: url("data:image/png;base64,");
}

.pos_msg view:nth-child(2) {
    background-image: url("data:image/png;base64,");
}

.pos_msg view:nth-child(3) {
    background-image: url("data:image/png;base64,");
}

.intro_list {
    border-bottom: 1px solid #EEEEEE;
    padding-bottom: 24rpx;
}

.intro_list view {
    font-size: 28rpx;
    /* line-height: 58rpx; */
    color: #555555;
}

.intro_list {
    font-size: 28rpx;
    line-height: 44rpx;
    color: #555555;
}

.work_time {
    font-size: 28rpx;
    line-height: 44rpx;
    color: #555555;
}

.pos_time {
    border-bottom: 1px solid #EEEEEE;
    padding-bottom: 24rpx;
}

.bornone {
    border: none;
}

.intro_place {
    border-top: 26rpx solid #f5f5f5;
    padding: 24rpx;
}

.placetitle {
    font-size: 28rpx;
    line-height: 56rpx;
    color: #555555;
}
.placetitle span{
    color: red;
}
.baiodumap {
    width: 100%;
    height: 370rpx;
    margin-left: 0rpx;
    margin-top: 10rpx;
    margin-bottom: 108rpx;
    border-radius: 20rpx;
    overflow: hidden;
}

.pos_btngroup {
    border-top: 1px solid #EEEEEE;
}

.pos_btngroup view {
    float: left;
    width: 50vw;
    text-align: center;
    line-height: 88rpx;
}

.pos_btngroup view:first-child {
    font-size: 36rpx;
    color: #AAAAAA;
    background: #ffffff;
}

.pos_btngroup::after {
    content: "";
    display: block;
    clear: both;
}

.pos_btngroup view:last-child {
    font-size: 36rpx;
    font-weight: bold;
    color: #111111;
    background: #FFCF08;
}

.pos_btngroup {
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    margin-top: 64rpx;
}

.diacontent {
    padding: 24rpx;
}

.diatitle {
    font-size: 32rpx;
    font-weight: bold;
    line-height: 84rpx;
    color: #111111;
}
.diatitle span{
    font-size: 28rpx;
    line-height: 40rpx;
    color: #AAAAAA;
}
.diatop {
    border-bottom: 1px solid #EEEEEE;
    padding-bottom: 24rpx;
}

.diaage {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.diaage view {
    width: 188rpx;
    font-size: 28rpx;
    font-weight: bold;
    line-height: 64rpx;
    color: #111111;
    background: #EEEEEE;
    text-align: center;
    border-radius: 8rpx;
}
.van-radio {
    justify-content: space-between;
}
.van-radio__label{
    line-height: 34rpx!important;
}
.radiogroup .van-radio{
    height: 70rpx;
}
.diacneter{
    border-bottom: 1px solid #EEEEEE;
    padding-bottom: 24rpx;
}
.van-radio{
    color: #555555!important;
    font-size: 28rpx!important;
}
.btngroup{
    display: flex;
    justify-content: space-between;
    border-top: 1px solid #EEEEEE;
    box-sizing: border-box;
}
.btngroup view{
    width: 50vw;
    line-height: 88rpx;
    text-align: center;
}
.btngroup view:first-child{
    color: #AAAAAA;
    font-size: 36rpx;
}
.btngroup view:last-child{
    color: #111111;
    font-size: 36rpx;
    background:#FFCF08 ;
    font-weight: bold;
}
.pb20{
    padding-bottom: 26rpx;
}
.pb0{
    padding-bottom: 0px;
}